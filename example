#include <stdio.h>

int main(void){
    
    int i;
    int user;
    int x = 0;
    int y = 0; 

    printf("Pick a number from 1-10\n");
    scanf("%d", &user);
    
    for (i = 0; i < 11; i++) {
        printf("%dx%d = %d\n", x, user, y);
        x = x + 1;
        y = user*x;
    }
    
    return 0;

    
}
